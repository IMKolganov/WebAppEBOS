@page "/ManageCustomers/home"
@model WebAppEBOS.Views.ManageCustomers.IndexModel
<div class="container">
    <div class="row">
        <div class="col-md-4 offset-md-4">
            @* <a asp-page="./create" class="btn btn-primary">Create New Cake</a> *@
        </div>
    </div>
    <div class="row">
        @foreach (var item in Model.Customers)
        {
            <div class="col-md-3 m-3">
                <div class="card" style="width: 18rem;">
                    <div class="card-body">
                        <h5 class="card-title">@item.Name</h5>
                        <h6 class="card-subtitle mb-2 text-muted">@item.Path $</h6>
                        <p class="card-text">@item.RefAppId</p>
                        @* <a asp-page="./edit" asp-route-id="@item.Id" class="btn btn-primary">Edit</a> *@
                        @* <a asp-page="./delete" asp-route-id="@item.Id" class="btn btn-danger">Delete</a> *@
                    </div>
                </div>
            </div>
        }
    </div>
</div>
